{
  "extends": ["config:base", "group:dotNetCore"],
  "regexManagers": [
    {
      "fileMatch": ["(^|/)ContainerApps\\.cs$"],
      "matchStrings": ["Image = \"(?<depName>.*?):(?<currentValue>.*?)\",( |\\n)"],
      "datasourceTemplate": "docker"
    }
  ],
  "packageRules": [
    {
      "extends": "monorepo:dotnet",
      "groupName": ".NET",
      "additionalBranchPrefix": ""
    },
  ],
  "masterIssue": true
}
